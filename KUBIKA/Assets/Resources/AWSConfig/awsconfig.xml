<?xml version="1.0" encoding="utf-8"?>

<aws
    region="eu-west-3"
    correctForClockSkew="true">

  //this prints all outputs to the debug.log console
  <logging
      logTo="UnityLogger"
      logResponses="Always"
      logMetrics="true"
      logMetricsFormat="JSON"/>

  <s3 useSignatureVersion4="true" />

  <mobileAnalytics
		sessionTimeout = "5"
		maxDBSize = "5242880"
		dbWarningThreshold = "0.9"
		maxRequestSize = "102400"
		allowUseDataNetwork = "true"
	/>

</aws>